{
  "name": "telegram-proxy-service",
  "version": "0.1.0",
  "description": "Proxy service for handling WebSocket connections to Telegram",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@communicator/common": "workspace:*",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
