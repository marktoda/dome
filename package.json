{
  "name": "dome",
  "version": "1.0.0",
  "main": "./dist/cli/index.js",
  "bin": {
    "dome": "./dist/cli/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "pnpm build && pnpm start",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/cli/index.js",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "cli:link": "npm link"
  },
  "keywords": [
    "ai",
    "mastra",
    "cli",
    "dome"
  ],
  "author": "marktoda",
  "license": "MIT",
  "description": "Dome - AI assistant powered by Mastra framework",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marktoda/dome.git"
  },
  "homepage": "https://github.com/marktoda/dome#readme",
  "type": "module",
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.23",
    "@lancedb/lancedb": "^0.21.1",
    "@mastra/core": "^0.15.2",
    "@mastra/libsql": "^0.13.7",
    "@mastra/loggers": "^0.10.9",
    "@mastra/pg": "^0.14.5",
    "@mastra/rag": "^1.2.2",
    "ai": "^5.0.29",
    "apache-arrow": "^18.0.0",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "fast-glob": "^3.3.3",
    "gray": "^0.0.6",
    "gray-matter": "^4.0.3",
    "ink": "^6.0.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^12.7.0",
    "p-map": "^7.0.3",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.1.0",
    "uuid": "^11.1.0",
    "zod": "3.25.49",
    "zod-config": "^1.1.2"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/cli/index.js"
    }
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "mastra": "^0.10.12",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "why-is-node-running": "^3.2.2"
  }
}
