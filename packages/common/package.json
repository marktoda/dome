{
  "name": "@dome/common",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./errors/ServiceError": {
      "types": "./dist/errors/ServiceError.d.ts",
      "import": "./dist/errors/ServiceError.js",
      "require": "./dist/errors/ServiceError.js"
    },
    "./middleware/enhancedAuthMiddleware": {
      "types": "./dist/middleware/enhancedAuthMiddleware.d.ts",
      "import": "./dist/middleware/enhancedAuthMiddleware.js",
      "require": "./dist/middleware/enhancedAuthMiddleware.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@dome/errors": "workspace:*",
    "hono": "^4.0.0",
    "nanoid": "^3.3.4",
    "pino": "^9.6.0",
    "pino-http": "^8.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "^8.38.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.0.4",
    "vitest": "^3.1.3"
  }
}
