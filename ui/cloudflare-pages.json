{
  "build": {
    "command": "npm run pages:build",
    "output_directory": ".vercel/output/static",
    "root_directory": "ui"
  },
  "compatibility_date": "2024-09-23",
  "compatibility_flags": ["nodejs_compat"],
  "env": {
    "NEXT_PUBLIC_BASE_URL": { "value": "https://67143919.dome-8dm.pages.dev" },
    "NEXTAUTH_URL": { "value": "https://67143919.dome-8dm.pages.dev" },
    "NEXTAUTH_SECRET": { "value": "your-production-secret-value-should-be-set-in-environment" },
    "NEXTAUTH_JWT_MAX_AGE": { "value": "2592000" },
    "NEXT_PUBLIC_API_URL": { "value": "https://dome-api.chatter-9999.workers.dev" },
    "GITHUB_CLIENT_ID": { "value": "[Add your GitHub OAuth client ID]" },
    "GITHUB_CLIENT_SECRET": { "value": "[Add your GitHub OAuth client secret]" },
    "NOTION_CLIENT_ID": { "value": "[Add your Notion OAuth client ID]" },
    "NOTION_CLIENT_SECRET": { "value": "[Add your Notion OAuth client secret]" }
  },
  "routes": [
    {
      "pattern": "^/_next/static/.*",
      "cache": {
        "browser_ttl": 31536000,
        "edge_ttl": 31536000
      }
    },
    {
      "pattern": "^/static/.*",
      "cache": {
        "browser_ttl": 31536000,
        "edge_ttl": 31536000
      }
    },
    {
      "pattern": "^/favicon.ico",
      "cache": {
        "browser_ttl": 86400,
        "edge_ttl": 86400
      }
    },
    {
      "pattern": "^/robots.txt",
      "cache": {
        "browser_ttl": 86400,
        "edge_ttl": 86400
      }
    },
    {
      "pattern": "^/.*",
      "cache": {
        "browser_ttl": 0,
        "edge_ttl": 0
      }
    }
  ]
}
